<!DOCTYPE html>
<html lang="en" class="[scrollbar-gutter:stable]">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="csrf-token" content={get_csrf_token()} />
    <.live_title default="KasiCrm" suffix=" Â· Phoenix Framework">
      {assigns[:page_title]}
    </.live_title>
    <link phx-track-static rel="stylesheet" href={~p"/assets/app.css"} />

    <!--Favicons-->
    <link rel="apple-touch-icon" sizes="180x180" href={~s"/images/favicons/apple-touch-icon.png"}>
    <link rel="icon" type="image/png" sizes="32x32" href={~s"/images/favicons/favicon-32x32.png"}>
    <link rel="icon" type="image/png" sizes="16x16" href={~s"/images/favicons/favicon-16x16.png"}>
    <link rel="shortcut icon" type="image/x-icon" href={~s"/images/favicons/favicon.ico"}>
    <link rel="manifest" href={~s"/images/favicons/manifest.json"}>
    <meta name="msapplication-TileImage" content={~s"/images/favicons/mstile-150x150.png"}>
    <meta name="theme-color" content="#ffffff">
    <script src={~p"/assets/vendor/simplebar/simplebar.min.js"}></script>
    <%!-- <script src={~p"/assets/js/config.js"}></script> --%>

    <!--    Stylesheets-->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="">
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;600;700;800;900&amp;display=swap" rel="stylesheet">
    <link href={~p"/assets/vendor/simplebar/simplebar.min.css"} rel="stylesheet">
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.8/css/line.css">
    <link href={~p"/assets/css/theme-rtl.min.css"} type="text/css" rel="stylesheet" id="style-rtl">
    <link href={~p"/assets/css/theme.min.css"} type="text/css" rel="stylesheet" id="style-default">
    <link href={~p"/assets/css/user-rtl.min.css"} type="text/css" rel="stylesheet" id="user-style-rtl">
    <link href={~p"/assets/css/user.min.css"} type="text/css" rel="stylesheet" id="user-style-default">

    <script defer phx-track-static type="text/javascript" src={~p"/assets/app.js"}>
    </script>
  </head>
  <body class="bg-white">
    <.admin_sidebar />
    <%!-- current_user={assigns[:current_user]} --%>
    
    {@inner_content}

    <!--Load last-->
    <script src={~s"/assets/vendor/popper/popper.min.js"}></script>
    <script src={~s"/assets/vendor/bootstrap/bootstrap.min.js"}></script>
    <script src={~s"/assets/vendor/anchorjs/anchor.min.js"}></script>
    <script src={~s"/assets/vendor/is/is.min.js"}></script>
    <script src={~s"/assets/vendor/fontawesome/all.min.js"}></script>
    <script src={~s"/assets/vendor/lodash/lodash.min.js"}></script>
    <script src={~s"/assets/vendor/list.js/list.min.js"}></script>
    <script src={~s"/assets/vendor/feather-icons/feather.min.js"}></script>
    <script src={~s"/assets/vendor/dayjs/dayjs.min.js"}></script>
    <%!-- <script src={~p"/assets/js/phoenix.js"}></script> --%>
  </body>
</html>
